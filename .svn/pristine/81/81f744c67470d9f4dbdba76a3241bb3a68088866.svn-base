package kr.or.ddit.project.vo;

import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class ScheduleVO {
	private String schdlNo; //일정번호
	private String pjtNo; //프로젝트 번호
	private String schdlCtgrCd; //일정분류코드
	private String schdlTtl; //일정제목
	private String schdlCn; //일정 내용
	private String schdlSrtDt; //일정 시작일
	private String schdlNdDt; //일정 종료일
	private String schdlWrtrNm; //일정 작성자
	public String getSchdlNo() {
		return schdlNo;
	}
	public void setSchdlNo(String schdlNo) {
		this.schdlNo = schdlNo;
	}
	public String getPjtNo() {
		return pjtNo;
	}
	public void setPjtNo(String pjtNo) {
		this.pjtNo = pjtNo;
	}
	public String getSchdlCtgrCd() {
		return schdlCtgrCd;
	}
	public void setSchdlCtgrCd(String schdlCtgrCd) {
		this.schdlCtgrCd = schdlCtgrCd;
	}
	public String getSchdlTtl() {
		return schdlTtl;
	}
	public void setSchdlTtl(String schdlTtl) {
		this.schdlTtl = schdlTtl;
	}
	public String getSchdlCn() {
		return schdlCn;
	}
	public void setSchdlCn(String schdlCn) {
		this.schdlCn = schdlCn;
	}
	public String getSchdlSrtDt() {
		return schdlSrtDt;
	}
	public void setSchdlSrtDt(String schdlSrtDt) {
		this.schdlSrtDt = schdlSrtDt;
	}
	public String getSchdlNdDt() {
		return schdlNdDt;
	}
	public void setSchdlNdDt(String schdlNdDt) {
		this.schdlNdDt = schdlNdDt;
	}
	public String getSchdlWrtrNm() {
		return schdlWrtrNm;
	}
	public void setSchdlWrtrNm(String schdlWrtrNm) {
		this.schdlWrtrNm = schdlWrtrNm;
	}
	
	@Override
	   public String toString() {
	      return ToStringBuilder.reflectionToString(this, ToStringStyle.JSON_STYLE);
	   }
}
