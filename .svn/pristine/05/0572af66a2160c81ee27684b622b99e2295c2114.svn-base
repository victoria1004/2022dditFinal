package kr.or.ddit.project.vo;

import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class KeywordVO {
	private String kwdNo;
	private String kwdNm;
	private String pjtNo;
	public String getKwdNo() {
		return kwdNo;
	}
	public void setKwdNo(String kwdNo) {
		this.kwdNo = kwdNo;
	}
	public String getKwdNm() {
		return kwdNm;
	}
	public void setKwdNm(String kwdNm) {
		this.kwdNm = kwdNm;
	}
	public String getPjtNo() {
		return pjtNo;
	}
	public void setPjtNo(String pjtNo) {
		this.pjtNo = pjtNo;
	}
	
	@Override
	   public String toString() {
	      return ToStringBuilder.reflectionToString(this, ToStringStyle.JSON_STYLE);
	   }
}