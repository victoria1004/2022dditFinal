package kr.or.ddit.project.vo;

public class KanbanCardVO {

	
	private String kbCardNo;	 //카드 번호
	private String kbNo;		 //칸반 번호
	private String kbCardIdxNo;  //인덱스 번호
	private String kbCardWrtrNm; //이름
	private String kbCardTtl;	 //카드 제목
	private String kbCardCn;	 //카드 내용
	private String kbCardStrtDt; //시작날
	private String kbCardEndDt;  //종료날
	private String kbPrgrs;		 //진척도
	
	
	
	public String getKbCardNo() {
		return kbCardNo;
	}
	public void setKbCardNo(String kbCardNo) {
		this.kbCardNo = kbCardNo;
	}
	public String getKbNo() {
		return kbNo;
	}
	public void setKbNo(String kbNo) {
		this.kbNo = kbNo;
	}
	public String getKbCardIdxNo() {
		return kbCardIdxNo;
	}
	public void setKbCardIdxNo(String kbCardIdxNo) {
		this.kbCardIdxNo = kbCardIdxNo;
	}
	public String getKbCardWrtrNm() {
		return kbCardWrtrNm;
	}
	public void setKbCardWrtrNm(String kbCardWrtrNm) {
		this.kbCardWrtrNm = kbCardWrtrNm;
	}
	public String getKbCardTtl() {
		return kbCardTtl;
	}
	public void setKbCardTtl(String kbCardTtl) {
		this.kbCardTtl = kbCardTtl;
	}
	public String getKbCardCn() {
		return kbCardCn;
	}
	public void setKbCardCn(String kbCardCn) {
		this.kbCardCn = kbCardCn;
	}
	public String getKbCardStrtDt() {
		return kbCardStrtDt;
	}
	public void setKbCardStrtDt(String kbCardStrtDt) {
		this.kbCardStrtDt = kbCardStrtDt;
	}
	public String getKbCardEndDt() {
		return kbCardEndDt;
	}
	public void setKbCardEndDt(String kbCardEndDt) {
		this.kbCardEndDt = kbCardEndDt;
	}
	public String getKbPrgrs() {
		return kbPrgrs;
	}
	public void setKbPrgrs(String kbPrgrs) {
		this.kbPrgrs = kbPrgrs;
	}
	
	
	@Override
	public String toString() {
		return "KanbanCardVO [kbCardNo=" + kbCardNo + ", kbNo=" + kbNo + ", kbCardIdxNo=" + kbCardIdxNo
				+ ", kbCardWrtrNm=" + kbCardWrtrNm + ", kbCardTtl=" + kbCardTtl + ", kbCardCn=" + kbCardCn
				+ ", kbCardStrtDt=" + kbCardStrtDt + ", kbCardEndDt=" + kbCardEndDt + ", kbPrgrs=" + kbPrgrs + "]";
	}
	
	
	
	
	
	
}
