		url: '/ddit/function/reservation_func.php?divide=key_toggle',
		data: {
			drl_idx : drl_idx,
			div: div,
			value : flag ? 'Y' : 'N'
		},
		async: false,
		success: function(data){
			console.log(data);
// 			location.reload();
		}
	});
}
