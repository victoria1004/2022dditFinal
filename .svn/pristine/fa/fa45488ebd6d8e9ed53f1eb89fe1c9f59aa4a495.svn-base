	function fn_insert(){
		
		//ìµëª…ì—¬ë¶€ ì„ íƒ í™•ì¸
		var cbox = $('input:checkbox').prop('checked');
		var flag = "";
		if (cbox){ //ì²´í¬ ì‹œ
