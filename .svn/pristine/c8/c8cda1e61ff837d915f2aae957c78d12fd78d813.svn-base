.bodyMainDiv{
	margin-top:100px;
	display: flex;
	
}

.leftDiv {
/* 	width:calc(100%-800px); */
	height:auto;
}

.rightDiv {
/* 	border : 1px solid blue; */
	display : flex;
	flex-direction: column;
/* 	margin-left : 15px; */
	padding-left : 15px;
	width:1200px;
	height:auto;
	position:relative;
	
	
}

.top-searchbar {
	text-align : center;
	padding: 15px 15px;
    background-color: #fff;
    border-radius: 0.25rem;
    width:1200px;
    height:40px;
    border: 1px solid #e0e6ed;
    border-radius: 6px;
    box-shadow: 4px 6px 10px -3px #bfc9d4;
}
.filelist {
	margin : 5px;
	text-align : center;
	padding: 15px 15px;
    background-color: #fff;
    border-radius: 0.25rem;
    width:155px;
    height:155px;
    
/*     border: 1px solid #e0e6ed; */
    border-radius: 6px;
    box-shadow: 4px 6px 10px -3px #bfc9d4;
}
.allFiles {
	overflow : hidden;
	display: flex;
	flex-flow: row wrap;
	
}
#fileZip {
	height : 800px;
	
}
.file-title {
	margin-top : 8px;
	text-align : center;
	font-weight: bold;	
/* 	font-size:0.9em; */
}

.file-size {
/* 	margin-top : 2.5px; */
	text-align : center;
}

.dropdownbox {
	display : flex;
	justify-content: flex-end;
}

.menu-top {
	margin-left : 80px;
}

.btnDiv {
	margin-left : 48px;
}
.searchDiv {
	width : 95%;
}
.iconBtn {
	margin-left : 5px;
	color: #888ea8;
	width : 50px;
	height : 45px;
	display: flex;
    align-items: center;
    padding: 0.375rem 0.75rem;
    margin-bottom: 0;
    font-size: 1rem;
    font-weight: 400;
    line-height: 1.5;
    color: #495057;
    text-align: center;
    white-space: nowrap;
    border-radius: 0.25rem;
    border: 1px solid #bfc9d4;
    background-color: #f1f2f3;
}
.searchNuploadBtn {
	display : flex;
	height : 50px;
}

.parent-folder {
	display: inline-block;
    text-align: center;
    width: 50px; 
    height : 80px;
    margin-right: 0.25rem;
    vertical-align: middle;
    font-size: 1rem;
/*     color: #2196f3; */
    cursor : pointer;
    padding : 5px;
}

.disFlex {
	display: flex;
	flex-direction: column;
}
#arrayList {
	border: 1px solid #f1f2f3;
	width : 442px;
	height : 35px;
	margin : 2px;
	padding: 5px;

}
.folderList {
	margin : 2px;
	width : 442px;
	height : 120px;
	background-color : #f1f2f3;
	padding : 5px;
	display: flex;
	flex-direction: row;
}
.top-folder {
	display: block;
}
.btm-folder {
	display: block;
	font-size: 0.8rem;
}

